{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "component/*": ["component/*"],
      "context/*": ["context/*"],
      "store/*": ["store/*"],
      "util/*": ["util/*"],
      "service/*": ["service/*"],
      "resource/*": ["resource/*"],
      "hook/*": ["hook/*"],
      "constant/*": ["constant/*"]
    },

    "sourceMap": true,
    "jsx": "react",
    "target": "es5",
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowJs": true,
    "outDir": "./built"
  },
  "include": ["**/*"]
}
